{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>puniyu/github-action//.github/renovate"],
  "packageRules": [
    {
      "groupName": "puniyu logger packages",
      "matchManagers": ["cargo"],
      "rangeStrategy": "auto",
      "schedule": ["at any time"],
      "labels": ["dependencies"],
      "platformAutomerge": true,
      "automerge": true,
      "automergeType": "pr"
    }
  ]
}